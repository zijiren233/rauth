# Server configuration
RAUTH_PORT=8080
RAUTH_ISSUER=rauth
RAUTH_SERVICE=registry
RAUTH_LOG_LEVEL=info

# Token configuration
RAUTH_PRIVATE_KEY=/path/to/token.key
RAUTH_TOKEN_EXPIRY=5m

# Kubernetes configuration (production)
RAUTH_SECRET_NAME=registry-credentials

# Mock mode (development/testing)
RAUTH_MOCK_MODE=false
RAUTH_MOCK_CONFIG=/path/to/mock-credentials.json
